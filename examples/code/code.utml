<% var pageTitle = "UTML - Code"; %>
<html>
<head>
  <title><%= pageTitle %></title>
</head>
<body>
  <h1><%= title %></h1>
  
  <ul>
    <% for(var i = 0; i < people.length; i++) { 
    	var person = people[i]; %>
      <li><%= person.name %> 
	      <% if (person.email) { %>&lt;<%= person.email %>&gt;<% } %></li>
    <% } %>
  </ul>
</body>
</html>
